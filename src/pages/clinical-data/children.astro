import InternalNavigation from "../../components/InternalNavigation.astro";
import ClinicalDataModal from "../../components/clinical-data/ClinicalDataModal.astro";
import DisabilityIndexModal from "../../components/clinical-data/DisabilityIndexModal.astro";
import Table from "../../components/Table/Table.astro";
import TableHeader from "../../components/Table/TableHeader.astro";
import TableData from "../../components/Table/TableData.astro";
import studyData from "../clinical-data/data/clinical-data-children.js";
import References from "../../components/References.astro";

const internalNavLinks = [
  { href: "hpp-related-rickets", label: "HPP-related rickets", active: true },
  { href: "mobility", label: "Mobility" },
  { href: "pain-disability", label: "Pain/disability" },
  { href: "strength-agility", label: "Strength/agility" },
];

const mainHeaders = [
  { label: "", sublabel: "" },
  { label: "Study 3<sup>1,2,7,14</sup>", subLabel: "ENB-006-09/ ENB-008-10" },
];
---

<Layout title="Children">
  <PageHero
    pageId="clinical-data"
    classNames="clinical-data-children__page-hero"
  >
    <h2 class="page-hero__subtitle">in children with hypophosphatasia</h2>
    <h1 class="page-hero__title">
      STRENSIQ helps build their framework and gets them moving<sup>1</sup>
    </h1>
  </PageHero>
  {
    internalNavLinks && (
      <InternalNavigation items={internalNavLinks} heading="Children" icon />
    )
  }
  <div class="clinical-data-children">
    <section
      id="hpp-related-rickets"
      class="section cdchildren-container cdchildren-container--rickets"
    >
      <div class="wrapper">
        <section
          class="cdchildren-container__content cdchildren-container__content--left"
        >
          <h2 class="color--dark-green cdchildren-container__title">
            STRENSIQ showed complete or substantial healing of HPP-related
            rickets<sup>1,2</sup>
          </h2>
          <HighlightInformation
            title="After 6 months<sup>2</sup>"
            footnote="of patients achieved complete or substantial healing of HPP-related rickets"
            className="highlight-information--contains-subcontent highlight-information--single"
            breakLineAfterFootnote={true}
          >
            <span slot="content">69<sub>%</sub></span>
            <span class="highlight-information__sub-content" slot="content"
              >(9/13)</span
            >
            <span slot="footnote"
              >as measured by the RGI-C scale<sup>*</sup></span
            >
          </HighlightInformation>
          <HighlightInformation
            title="After 7 years<sup>1</sup>"
            footnote="of patients achieved complete or substantial healing of HPP-related rickets"
            className="highlight-information--contains-subcontent highlight-information--single"
            breakLineAfterFootnote={true}
          >
            <span slot="content">100<sub>%</sub></span>
            <span class="highlight-information__sub-content" slot="content"
              >(12/12)<sup>†</sup></span
            >
            <span slot="footnote"
              >as measured by the RGI-C scale<sup>*</sup></span
            >
          </HighlightInformation>
          <p class="cdchildren-container__footnote">
            <sup>*</sup>The RGI-C scale quantifies change over time in
            mineralization and bone structure.
          </p>
          <p class="cdchildren-container__footnote">
            <sup>†</sup>12 patients entered the long-term extension of the
            trial.<sup>3</sup>
          </p>
        </section>
        <section
          class="cdchildren-container__content cdchildren-container__content--right"
        >
          <div class="radiographic-images-mobile">
            <p class="radiographic-images-mobile__title">Radiographic images</p>
            <AccordionMobile id="children-radiographics">
